{
    "name": "derekhamilton/oatmeal",
    "description": "PHP cookie management in a modern, object-oriented manner.",
    "keywords": ["php", "cookie", "manage", "setcookie"],
    "license": "MIT",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.1|7.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "DerekHamilton\\Oatmeal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DerekHamilton\\Tests\\Oatmeal\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DerekHamilton\\Oatmeal\\Providers\\OatmealServiceProvider"
            ]
        }
    }
}
